{
  "name": "zx-web-webpack",
  "version": "1.0.0",
  "description": "this is web-webpack demo based on react for zx",
  "main": "index.ts",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": " yarn dev",
    "dev": "cross-env NODE_ENV=development webpack serve --config ./build-scripts/webpack.config.babel.ts",
    "build": "cross-env NODE_ENV=production webpack --config ./build-scripts/webpack.config.babel.ts",
    "build:ana": "cross-env NODE_ENV=production BUNDLE_ANNALYZE=analysis webpack --config ./build-scripts/webpack.config.babel.ts",
    "build:dll": "webpack --config ./build-scripts/webpack.dll.config.babel.ts",
    "preinstall": "npx only-allow yarn",
    "prepare": "husky install"
  },
  "dependencies": {
    "antd": "^4.21.7",
    "axios": "^0.27.2",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/progress-bar-webpack-plugin": "^2.1.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "add-asset-html-webpack-plugin": "^5.0.2",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "connect-history-api-fallback": "^2.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^10.1.3",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.1",
    "only-allow": "^1.1.1",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-refresh": "^0.14.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "stylus": "^0.58.1",
    "stylus-loader": "^7.0.0",
    "terser-webpack-plugin": "^5.3.3",
    "type-fest": "^2.16.0",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  }
}
