{
  "name": "zx-web-webpack",
  "version": "1.0.0",
  "description": "this is web-webpack demo based on react for zx",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "start": " yarn dev",
    "dev": "cross-env NODE_ENV=development webpack serve --config ./build-scripts/webpack.config.babel.ts",
    "build": "cross-env  NODE_ENV=production webpack --config ./build-scripts/webpack.config.babel.ts",
    "build:ana": "cross-env NODE_ENV=production BUNDLE_ANNALYZE webpack --config ./build-scripts/webpack.config.babel.ts"
  },
  "dependencies": {
    "antd": "^4.21.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/progress-bar-webpack-plugin": "^2.1.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "babel-loader": "^8.2.5",
    "connect-history-api-fallback": "^2.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
